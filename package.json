{
  "name": "rosplata-back",
  "version": "0.2.5",
  "description": "",
  "author": "",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate": "sequelize db:migrate && npm run seed",
    "unmigrate": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "unseed": "sequelize db:seed:undo:all",
    "reseed": "npm run unseed && npm run seed",
    "migration:add": "sequelize migration:create",
    "translations:generate": "node ./scripts/generate-translations.js",
    "translations:apply": "node ./scripts/apply-translations.js",
    "translations:add": "node ./scripts/add-translation.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "firebase-admin": "^11.4.0",
    "isbot": "^3.6.10",
    "jsdom": "^21.1.1",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-firebase-jwt": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.26.0",
    "sequelize-typescript": "^2.1.5",
    "spdy": "^4.0.2",
    "ua-parser-js": "^1.0.35",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/sequelize": "^4.28.14",
    "@types/spdy": "^3.4.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.5.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
